		"SqlConection2Private_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlConection2Private'"
		},
		"StorajeJosele1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorajeJosele1'"
