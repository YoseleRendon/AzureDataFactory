{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFDemoJosele1"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"ExcelFileInputDataLakeGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ExcelFileInputDataLakeGen2'"
		},
		"LinkedServiceJoseleAzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkedServiceJoseleAzureDataLakeStorage1'"
		},
		"LinkedService_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedService_SQLDB'"
		},
		"ExcelFileInputDataLakeGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagejosele2.dfs.core.windows.net/"
		},
		"LinkedServiceJoseleAzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagejosele.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFDemoJosele1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"ExcelFileInputDataLakeGen2_accountKey": {
						"value": "[parameters('ExcelFileInputDataLakeGen2_accountKey')]"
					},
					"LinkedServiceJoseleAzureDataLakeStorage1_accountKey": {
						"value": "[parameters('LinkedServiceJoseleAzureDataLakeStorage1_accountKey')]"
					},
					"LinkedService_SQLDB_connectionString": {
						"value": "[parameters('LinkedService_SQLDB_connectionString')]"
					},
					"ExcelFileInputDataLakeGen2_properties_typeProperties_url": {
						"value": "[parameters('ExcelFileInputDataLakeGen2_properties_typeProperties_url')]"
					},
					"LinkedServiceJoseleAzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('LinkedServiceJoseleAzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}